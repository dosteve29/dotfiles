Description='A wireless connection for Temple University'
Interface=wlp3s0
Connection=wireless
Security=wpa-configsection
IP=dhcp
WPAConfigSection=(
	'ssid="tusecurewireless"'
	'scan_ssid=1'
	'key_mgmt=WPA-EAP'
	'identity="USERNAME"'
	'password="PASSWORD"'
	'eap=PEAP'
	'phase1="peaplabel=0"'
	'phase2="auth=MSCHAPV2"'
)
